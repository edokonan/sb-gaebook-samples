<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>dataManager</servlet-name>
    <servlet-class>gaebook.blob.DataManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dataManager</servlet-name>
    <url-pattern>/dataManager</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>upload</servlet-name>
    <servlet-class>gaebook.blob.Upload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>fileServer</servlet-name>
    <servlet-class>gaebook.blob.FileServer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileServer</servlet-name>
    <url-pattern>/fileServer</url-pattern>
  </servlet-mapping>

	<welcome-file-list>
		<welcome-file>dataManager</welcome-file>
	</welcome-file-list>
</web-app>
