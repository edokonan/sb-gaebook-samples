<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

  <servlet>
    <servlet-name>ImageDisplay</servlet-name>
    <servlet-class>gaebook.images.ImageDisplay</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageDisplay</servlet-name>
    <url-pattern>/imageDisplay</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ImageUpload</servlet-name>
    <servlet-class>gaebook.images.ImageUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ImageUpload</servlet-name>
    <url-pattern>/imageUpload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>dataManager</servlet-name>
    <servlet-class>gaebook.images.DataManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dataManager</servlet-name>
    <url-pattern>/dataManager</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>compositeTest</servlet-name>
    <servlet-class>gaebook.images.CompositeTest</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>compositeTest</servlet-name>
    <url-pattern>/compositeTest</url-pattern>
  </servlet-mapping>
  
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
